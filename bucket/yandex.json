{
    "version": "25.12.4.1216",
    "description": "The web browser from Yandex. Yandex Browser is a browser that combines a minimal design with sophisticated technology to make the web faster, safer, and easier.",
    "homepage": "https://browser.yandex.com/",
    "license": "Unknown",
    "architecture": {
        "64bit": {
            "url": "https://download.cdn.yandex.net/browser/update/25_12_4_1216_100679_w_s_r/yandex.exe",
            "hash": "61ee5e35833a854b039bdef594936f834c05dfd8a3ed1a7e2c28f60a5376ed79"
        }
    },
    "installer": {
        "script": [
            "Expand-7zipArchive \"$dir\\yandex.exe\" -Removal",
            "Expand-7zipArchive \"$dir\\browser.7z\" -ExtractDir 'Browser-bin' -Removal"
        ]
    },
    "bin": [
        [
            "browser.exe",
            "yandex",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "shortcuts": [
        [
            "browser.exe",
            "Yandex Browser",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "persist": "User Data",
    "checkver": {
        "url": "https://api.browser.yandex.ru/update-info/browser/yandex/win-yandex.rss?version=$version",
        "regex": "<guid.*?>.*update/(?<build64>.+?)/.*</guid.*?>[\\s\\S]+<version:version>(?<version>.+?)</version:version>",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.cdn.yandex.net/browser/update/$matchBuild64/yandex.exe"
            }
        }
    }
}
