{
    "version": "25.4.4.526",
    "description": "The web browser from Yandex. Yandex Browser is a browser that combines a minimal design with sophisticated technology to make the web faster, safer, and easier.",
    "homepage": "https://browser.yandex.com/",
    "license": "Unknown",
    "architecture": {
        "64bit": {
            "url": "https://download.cdn.yandex.net/browser/update/25_4_4_526_80829_w_b_r/beta-custo-int.exe",
            "hash": "0f8e46d0c04ac0197b0a4ebd986aeff71d5c0ed6adf654b9809cc26970ab36ad"
        }
    },
    "installer": {
        "script": [
            "Expand-7zipArchive \"$dir\\beta-custo-int.exe\" -Removal",
            "Expand-7zipArchive \"$dir\\browser.7z\" -ExtractDir 'Browser-bin' -Removal"
        ]
    },
    "bin": [
        [
            "browser.exe",
            "yandex",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "shortcuts": [
        [
            "browser.exe",
            "Yandex Browser Beta",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "persist": "User Data",
    "checkver": {
        "url": "https://api.browser.yandex.com/update-info/browser/int-beta/win-int.rss?version=$version",
        "regex": "<guid.*?>.*update/(?<build64>.+?)/.*</guid.*?>[\\s\\S]+<version:version>(?<version>.+?)</version:version>",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.cdn.yandex.net/browser/update/$matchBuild64/beta-custo-int.exe"
            }
        }
    }
}
