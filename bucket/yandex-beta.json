        "url": "https://api.browser.yandex.com/update-info/browser/int-beta/win-int.rss?version=0.0.0.0",
        "regex": "<guid64>.*update/(?<build64>.+?)/.*</guid64>[\\s\\S]+<version:version>(?<version>.+?)</version:version>",
